@{ ViewData["Title"] = ViewBag.NameControl; }

<h1>@ViewBag.NameControl</h1>

<div style="display: flex" ;>
    @{
        if (ViewBag.Result is string)
        {
            <b class="m-2">@ViewBag.Object </b>
            <pre class="m-2" style="font-family:Arial; font-size:16px;">@ViewBag.Result</pre>
        }
        else if (ViewBag.Result is string[])
        {
            <b class="m-2">@ViewBag.Object </b> <br/>
            var array = ViewBag.Result as string[];
            for (int i = 0; i < array.Length; i++)
            {
                <pre class="m-2" style="font-family:Arial; font-size:16px;">@array[i]</pre><br />
            }
        }
    }
</div>